{
  "manifest_version": 2,
  "name": "BBellad",
  "version": "0.1.1",
  "description": "Replaces all images with Blondinbella",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end",
      "js": ["bbellad.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
